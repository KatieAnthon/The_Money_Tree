{
  "expo": {
    "entryPoint": "./App.js",
    "name": "TheMoneyTree",
    "slug": "TheMoneyTree",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/icon.png",
    "userInterfaceStyle": "automatic",
    "scheme":"com.example.themoneytree",
    "splash": {
      "image": "./app/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.example.themoneytree",
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.example.themoneytree"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./app/assets/favicon.png"
    },
    "plugins": [
      "expo-secure-store",
      [
        "react-native-auth0",
        {
          "domain": "dev-e1o8urvr8aif2req.uk.auth0.com"
        }
      ]
    ]
  }
}
